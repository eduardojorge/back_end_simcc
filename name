		<script type="text/javascript">
			var widget;
		    var verifyCallback = function(response) {
		    	$("#tokenCaptchar").attr('value', grecaptcha.getResponse());
		    	$('#submitBtn').removeAttr('disabled');
		    	
		    	$("#submitBtn").click(function(){
		    		$( "#div_gif" ).append( "<img style=\"-webkit-user-select: none\" src=\"/buscatextual/images/gif/ajax-loader.gif\">" );
		    		visualizarCurriculo();
		    	});
			};
			
			var onloadCallback = function() {
				widget = grecaptcha.render('divCaptcha', {
					'sitekey' : '6LeDv6QUAAAAAP_ZK5AXrsNRQjxfbjgZLV5_YHyy',
					'callback' : verifyCallback,
					'theme' : 'light'
				});
			};
		
			function visualizarCurriculo(){
				var form = document.forms[0];
				form.submit();
			}
	
			setTimeout(function() {
				$('#div_gif').remove();
			}, 10000);
		</script>

